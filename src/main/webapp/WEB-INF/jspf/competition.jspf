<h2>Compétitions</h2>
<table class="table table-striped">
	<thead>
		<tr>
			<th>Date</th>
			<th>Nom</th>
			<th>Ville</th>
			<th>Adresse</th>
			<th>Département</th>
		</tr>
	</thead>
	<tbody>
		<c:forEach items="${list}" var="competition">
			<tr>
				<td>${competition.date}</td>
				<td>${competition.nom}</td>
				<td>${competition.ville}</td>
				<td>${competition.adresse}</td>
				<td>${competition.departement}</td>
			</tr>
		</c:forEach>

	</tbody>
</table>